	"Roadmap": {
		"title": "路線圖",
		"list": [
			{
				"time": "Phase 1: 2019.Q3-2020.Q4",
				"title": "引力:",
				"introduce": [
					"超輕驗證協議",
					"MAP-PoS共識",
					"MAP-VM和SMART系統"
				]
			},
			{
				"time": "Phase 2: 2021.Q3",
				"title": "电磁:",
				"introduce": [
					"測試網上線，啟動與eth的去中心化跨鏈測試",
					"開展公測，邀請更多用戶加入節點參與測試網公測"
				]
			},
			{
				"time": "Phase 3: 2021.Q4",
				"title": "相互作用:",
				"introduce": [
					"主網正式上線，支持多鏈間去中心化跨鏈",
					"支持跨鏈swap和更多defi項目加入"
				]
			},
			{
				"time": "Phase 4: 2022.Q3",
				"title": "大統一:",
				"introduce": [
					"接入更多公鏈，優化協議，為跨鏈預言機、借貸等各種DApp提供支持",
					"支持BTC等非智能合約主鏈跨鏈，為非智能合約主鏈提供exchange和swap支持"
				]
			},
			{
				"time": "Phase 5: 2023.Q2",
				"title": "大統一:",
				"introduce": [
					"支持任意消息的去中心化跨鏈",
					"提供更完善的SDK，讓更多區塊鏈可以更方便接入跨鏈"
				]
			}
		]
	},
